import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int[] arr={2,0,1,2,0,0,1,2};
		ArrayList<Integer> newarr=new ArrayList<>();
		for(int x:arr){
		    if(x==0){
		        newarr.add(x);
		    }
		}
		for(int x:arr){
		    if(x==1){
		        newarr.add(x);
		    }
		}
		for(int x:arr){
		    if(x==2){
		        newarr.add(x);
		    }
		}
		for(int i=0;i<newarr.size();i++){
		    System.out.print(newarr.get(i)+" ");
		}
	}
}
