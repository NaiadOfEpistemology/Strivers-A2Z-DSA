import java.util.*;
public class Main{
    public static void main(String[] args){
        int n,i,j;
        Scanner s=new Scanner(System.in);
        n=s.nextInt();
        for(i=1;i<=n;i++){
            char alpha=(char)('A'+n-i);
            for(j=1;j<=i;j++){
                System.out.print(alpha+" ");
                alpha++;
            }
            System.out.println();
        }
    }
}
OUTPUT : 
E 
D E 
C D E 
B C D E 
A B C D E 
