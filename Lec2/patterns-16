import java.util.*;
public class Main{
    public static void main(String[] args){
        int n,i,j;
        
        Scanner s=new Scanner(System.in);
        n=s.nextInt();
        for(i=1;i<=n;i++){
            char alpha=(char)('A'+i-1);
            for(j=1;j<=i;j++){
                System.out.print(alpha+" ");
            }
            System.out.println();
        }
    }
}
OUTPUT : 
A 
B B 
C C C 
D D D D 
E E E E E 
