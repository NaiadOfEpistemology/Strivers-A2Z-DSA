import java.util.*;
public class Main{
    public static void main(String[] args){
        int n,i,j;
        
        Scanner s=new Scanner(System.in);
        n=s.nextInt();
        for(i=n;i>=1;i--){
            char alpha='A';
            for(j=1;j<=i;j++){
                System.out.print(alpha+" ");
                alpha++;
            }
            System.out.println();
        }
    }
}
OUTPUT :
A B C D E 
A B C D 
A B C 
A B 
A 
