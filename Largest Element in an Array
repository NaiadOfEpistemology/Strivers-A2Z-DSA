import java.util.*;

public class LArray {
    public static void main(String[] args){
        int[] arr={99, 34, 5, 1, 45, 89};
        int large=arr[0];
        for(int i=0;i<arr.length;i++){
            if(arr[i]>large){
                large=arr[i];
            }
        }
        System.out.println("Largest element : "+large);
    }
    
}

OUTPUT : 
Largest element : 99
