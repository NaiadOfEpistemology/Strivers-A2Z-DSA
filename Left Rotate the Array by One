public class Main{
    public static void main(String[] args){
        int[] arr={99, 34, 5, 1, 45, 89};
        int temp;
        for(int i=1;i<arr.length;i++){
            temp=arr[i];
            arr[i]=arr[i-1];
            arr[i-1]=temp;
        }
        for(int i=0;i<arr.length;i++){
            System.out.print(arr[i]+" ");
        }
    }
}

OUTPUT : 
34 5 1 45 89 99 
