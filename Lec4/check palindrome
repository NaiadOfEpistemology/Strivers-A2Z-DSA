import java.util.*;
public class Main{
	public static void main(String[] args){
		int count=0,num,rem,rev=0;
		Scanner s=new Scanner(System.in);
		num=s.nextInt();
		int onum=num;
		while(num>0){
		    rem=num%10;
		    rev=rev*10+rem;
		    num=num/10;
		}
		if(onum==rev){
		    System.out.println("Palindrome! "+rev);
		}
		else{
		    System.out.println("Not a Palindrome  "+rev);
		}
	}
}
OUTPUT : 
12345
Not a Palindrome  54321
12121
Palindrome! 12121
