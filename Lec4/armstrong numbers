import java.util.*;
public class Main{
	public static void main(String[] args){
		int num,b,rem,arm=0;
		String s;
		Scanner scn=new Scanner(System.in);
		num=scn.nextInt();
		int onum=num;
		int l=String.valueOf(num).length();
		while(num>0){
		    rem=num%10;
		    arm+=Math.pow(rem,l);
		    num=num/10;
		   }
		if(arm==onum){
		    System.out.println("Armstrong number");
		}
		else{
		    System.out.println("Not armstrong number");
		}
	}
}
OUTPUT : 
153
Armstrong number

100 
Not armstrong number
