public class Main
{
	public static void main(String[] args) {
		int[] arr={2,6,5,8,11};
		int target=11;
		int i,j;
		for(i=0;i<arr.length;i++){
		    for(j=i+1;j<arr.length;j++){
		        if (arr[i]+arr[j]==target){
		            System.out.println("YES");
		            return;
		        }
		    }
		}
		System.out.println("NO");
	}
}
