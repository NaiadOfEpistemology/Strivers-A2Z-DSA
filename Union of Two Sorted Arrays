import java.util.*;
public class Main {
    public static void main(String[] args) {
        Set<Integer> set1=new HashSet<>(Arrays.asList(99, 34, 5, 1, 45, 89));
        Set<Integer> set2=new HashSet<>(Arrays.asList(1, 2, 3, 45, 34, 100));
        Set<Integer> newset=new HashSet<>(set1);
        newset.addAll(set2);
        Set<Integer> sortedset=new TreeSet<>(newset);
        System.out.println(sortedset);
    }
}

OUTPUT :
[1, 2, 3, 5, 34, 45, 89, 99, 100]
